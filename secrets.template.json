{
  "_comment": "Copy this file to secrets.json and fill in your values. Never commit secrets.json!",
  "_instructions": "Use 'dotnet user-secrets set' or copy values to secrets.json in your user secrets folder",

  "ConnectionStrings": {
    "coralledger-db": "Host=localhost;Port=5432;Database=coralledger;Username=postgres;Password=YOUR_PASSWORD"
  },

  "GlobalFishingWatch": {
    "ApiToken": "YOUR_GFW_API_TOKEN_FROM_https://globalfishingwatch.org/our-apis/tokens",
    "Enabled": true
  },

  "ProtectedPlanet": {
    "ApiToken": "YOUR_WDPA_API_TOKEN_FROM_https://api.protectedplanet.net/request",
    "Enabled": true,
    "BaseUrl": "https://api.protectedplanet.net/v4/"
  },

  "AIS": {
    "Enabled": false,
    "Provider": "MarineTraffic",
    "ApiKey": "YOUR_AIS_PROVIDER_API_KEY",
    "BaseUrl": "https://services.marinetraffic.com/api/",
    "UpdateIntervalSeconds": 300,
    "BoundingBox": {
      "MinLon": -80.5,
      "MaxLon": -72.5,
      "MinLat": 20.5,
      "MaxLat": 27.5
    }
  },

  "MarineAI": {
    "Enabled": false,
    "ApiKey": "YOUR_OPENAI_OR_AZURE_OPENAI_API_KEY",
    "ModelId": "gpt-4o-mini",
    "UseAzureOpenAI": false,
    "AzureEndpoint": null,
    "MaxTokens": 1000,
    "Temperature": 0.3
  },

  "AzureStorage": {
    "ConnectionString": "YOUR_AZURE_STORAGE_CONNECTION_STRING",
    "ContainerName": "observation-photos",
    "Enabled": false,
    "MaxFileSizeBytes": 10485760
  }
}
