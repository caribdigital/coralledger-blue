{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GlobalFishingWatch": {
    "Enabled": false
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "InstanceName": "CoralLedger:",
    "Enabled": true,
    "MpaGeoJsonCacheTtlHours": 6,
    "NoaaBleachingCacheTtlHours": 12
  },
  "CoralReefWatch": {
    "UseMockData": true,
    "MockDataPath": "data/mock-bleaching-data.json"
  }
}
